program PStockV2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  UStockv2 in 'UStockv2.pas';

var
  MyData: TData;
  MyStock: TStock;
  name: string;

begin
writeln('Commence program');
writeln('What is the name of the program');
readln(name);
MyStock:= TStock.Create(name);
writeln('Success... i think');
readln;
end.
